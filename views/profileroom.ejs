<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>profileroom</title>
    <link rel="stylesheet" href="css/anime.css">
    <link rel="stylesheet" href="css/index.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css" integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
</head>
<body>
    <%- include('navbar')%>
    <div class="conteaner">
    <div class="img-room"></div>
    <div class="tsher"></div>
    <% Object.values(product).forEach(function(item){ %>
        <div class="nametag" data-name="<%= item.name %>" data-university="<%= item.university %>" data-group="<%= item.group %>" data-img="images/products/<%= item.img %>" data-age="<%= item.age %>" data-ds="<%= item.ds %>" data-warp="<%= item.warp %>" data-gender="<%= item.gender %>">
            <div class="img-nametag"><img src="images/products/<%= item.img%>" alt=""></div>
            <p><%= item.name %></p>
            <p><%= item.university %></p>
            <p class="last-chil"><%= item.group %></p>
        </div>
        <% }) %>
    
        <div id="popup" class="popup" style="display: none;">
            <div class="popup-content">
                <span class="close-btn">&times;</span>
                <div class="img-popup"><img id="popup-img" src="" alt=""></div>
                <p id="popup-name"></p>
                <p id="popup-university"></p>
                <p id="popup-group"></p>
                <p id="popup-gender"></p>
                <p id="popup-age"></p>
                <p id="popup-ds"></p>
                <p id="popup-warp"></p>
            </div>
        </div>

</body>
    <script src="js/drop.js"></script>
    <script src="js/gard.js"></script>
    <script src="js/Profile"></script>
</html>